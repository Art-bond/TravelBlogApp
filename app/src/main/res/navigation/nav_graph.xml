<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/fragmentLogin">

    <fragment
        android:id="@+id/fragmentLogin"
        android:name="ru.d3st.travelblogapp.login.FragmentLogin"
        android:label="FragmentLogin" >
        <action
            android:id="@+id/action_fragmentLogin_to_fragmentConsumer"
            app:destination="@id/fragmentConsumer" />
        <action
            android:id="@+id/action_fragmentLogin_to_fragmentCameraMap"
            app:destination="@id/fragmentCameraMap" />
    </fragment>
    <fragment
        android:id="@+id/fragmentConsumer"
        android:name="ru.d3st.travelblogapp.consumer.FragmentConsumer"
        android:label="fragment_consumer_fragment"
        tools:layout="@layout/fragment_consumer_fragment" />
    <fragment
        android:id="@+id/fragmentCameraMap"
        android:name="ru.d3st.travelblogapp.blogger.FragmentCameraMap"
        android:label="fragment_camera_map_fragment"
        tools:layout="@layout/fragment_camera_map_fragment" />
</navigation>